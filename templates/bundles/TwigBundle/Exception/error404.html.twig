{% extends 'base.html.twig' %}

{% block title %}
    Error 404 | {{ parent }}
{% endblock %}

{% block body %}
    <h1>Page not found</h1>

    <p>
        The requested page couldn't be located. Checkout for any URL
        misspelling or <a href="{{ path('main_homepage') }}">return to the homepage</a>.
    </p>

    <h3>{{ exception.message }}</h3>
{% endblock %}